<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    >
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/liquidAqueous">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Liquid aqueous</skos:prefLabel>
        <skos:definition>Specimen collected as liquid water or sensor emplaced to measure properties of water in sampled environment.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/snow">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Snow</skos:prefLabel>
        <skos:definition>Observation in, of or sample taken from snow.</skos:definition>
        <skos:historyNote>http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleMediumCV</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/equipment">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Equipment</skos:prefLabel>
        <skos:definition>An instrument, sensor or other piece of human-made equipment upon which a measurement is made, such as datalogger temperature or battery voltage.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/rock">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Rock</skos:prefLabel>
        <skos:definition>Specimen collected from a naturally occuring solid aggregate of one or more minerals.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php. Definition adapted from Wikipedia. See: https://en.wikipedia.org/wiki/Rock_(geology)</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/mineral">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Mineral</skos:prefLabel>
        <skos:definition>Specimen collected as a mineral.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/soil">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Soil</skos:prefLabel>
        <skos:definition>Specimen collected from soil or sensor emplaced to measure properties of soil. Soil includes the mixture of minerals, organic matter, gasses, liquids, and organisms that make up the upper layer of earth in which plants grow. </skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/notApplicable">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Not applicable</skos:prefLabel>
        <skos:definition>There is no applicable sampled medium.  </skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
        <skos:note>This term should be used in the case that there is no medium or medium does not apply in some context. </skos:note>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/sediment">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Sediment</skos:prefLabel>
        <skos:definition>Specimen collected from material broken down by processes of weathering and erosion and subsequently transported by the action of wind, water, or ice, and/or by the force of gravity acting on the particles. Sensors may also be emplaced to measure sediment properties.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php. Definition adapted from Wikipedia. See: https://en.wikipedia.org/wiki/Sediment</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/vegetation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Vegetation</skos:prefLabel>
        <skos:definition>The plants of an area considered in general or as communities, but not taxonomically.</skos:definition>
        <skos:historyNote>http://www.eionet.europa.eu/gemet</skos:historyNote>
        <skos:exactMatch>http://www.eionet.europa.eu/gemet/concept?ns=1&amp;cp=8922</skos:exactMatch>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/regolith">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Regolith</skos:prefLabel>
        <skos:definition>The entire unconsolidated or secondarily recemented cover that overlies more coherent bedrock, that has been formed by weathering, erosion, transport and/or deposition of the older material. The regolith thus includes fractured and weathered basement rocks, saprolites, soils, organic accumulations, volcanic material, glacial deposits, colluvium, alluvium, evaporitic sediments, aeolian deposits and ground water.&#13;
            Everything from fresh rock to fresh air.</skos:definition>
        <skos:exactMatch>http://crcleme.org.au/Pubs/Monographs/RegolithGlossary.pdf</skos:exactMatch>
        <skos:note>The term was introduced by Merrill (1897) who wrote of the incoherent mass of varying thickness covering the underlying rocks: “In places this covering is made up of material originating through rock weathering in-situ. In other instances it is of fragmental and more or less decomposed material drifted by wind, water or ice from other sources. This entire mantle of unconsolidated material, whatever its nature or origin, it is proposed to call the regolith, from the Greek words rhego), meaning a blanket, and lithos, a stone.”</skos:note>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/particulate">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Particulate</skos:prefLabel>
        <skos:definition>Specimen collected from particulates suspended in a paticulate-fluid mixture. Examples include particulates in water or air.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/tissue">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Tissue</skos:prefLabel>
        <skos:definition>Sample of a living organism's tissue or sensor emplaced to measure property of tissue.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/ice">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Ice</skos:prefLabel>
        <skos:definition>Sample collected as frozen water or sensor emplaced to measure properties of ice.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/air">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Air</skos:prefLabel>
        <skos:definition>Specimen collection of ambient air or sensor emplaced to measure properties of ambient air.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/organism">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Organism</skos:prefLabel>
        <skos:definition>Data collected about a species at organism level.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/gas">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Gas</skos:prefLabel>
        <skos:definition>Gas phase specimen or sensor emplaced to measure properties of a gas.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/liquidOrganic">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Liquid organic</skos:prefLabel>
        <skos:definition>Specimen collected as an organic liquid.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/other">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Other</skos:prefLabel>
        <skos:definition>Other.</skos:definition>
        <skos:historyNote>http://app.geosamples.org/reference/materials.php</skos:historyNote>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/habitat">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Habitat</skos:prefLabel>
        <skos:definition>A habitat is an ecological or environmental area that is inhabited by a particular species of animal, plant, or other type of organism.</skos:definition>
        <skos:historyNote>http://waterqualitydata.us/portal_userguide/#WQPUserGuide-Table2&#13;</skos:historyNote>
        <skos:note>Necessary to describe ecosystem properties, such as: birds per km2 of habitat&#13;
            &#13;
            STORET definition: The habitat conditions at the monitoring site; physical features of the area surrounding the monitoring location site.</skos:note>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocabulary.odm2.org/medium/unknown">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://vocabulary.odm2.org/medium"/>
        <skos:prefLabel>Unknown</skos:prefLabel>
        <skos:definition>The sampled medium is unknown.</skos:definition>
        <skos:historyNote>http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleMediumCV</skos:historyNote>
    </rdf:Description>
</rdf:RDF>